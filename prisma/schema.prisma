generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model RSVP {
  id          Int      @id @default(autoincrement())
  firstName   String?
  lastName    String?
  email       String?
  address     String?   @db.Text
  notes       String?   @db.Text
  going     Boolean?
  partyId   String?  @default(uuid())
}
